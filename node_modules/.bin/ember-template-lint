#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cvx/discourse/mass-pr/mass-pr-workspace/repo/node_modules/.pnpm/ember-template-lint@7.9.3/node_modules/ember-template-lint/bin/node_modules:/Users/cvx/discourse/mass-pr/mass-pr-workspace/repo/node_modules/.pnpm/ember-template-lint@7.9.3/node_modules/ember-template-lint/node_modules:/Users/cvx/discourse/mass-pr/mass-pr-workspace/repo/node_modules/.pnpm/ember-template-lint@7.9.3/node_modules:/Users/cvx/discourse/mass-pr/mass-pr-workspace/repo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cvx/discourse/mass-pr/mass-pr-workspace/repo/node_modules/.pnpm/ember-template-lint@7.9.3/node_modules/ember-template-lint/bin/node_modules:/Users/cvx/discourse/mass-pr/mass-pr-workspace/repo/node_modules/.pnpm/ember-template-lint@7.9.3/node_modules/ember-template-lint/node_modules:/Users/cvx/discourse/mass-pr/mass-pr-workspace/repo/node_modules/.pnpm/ember-template-lint@7.9.3/node_modules:/Users/cvx/discourse/mass-pr/mass-pr-workspace/repo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ember-template-lint/bin/ember-template-lint.js" "$@"
else
  exec node  "$basedir/../ember-template-lint/bin/ember-template-lint.js" "$@"
fi
